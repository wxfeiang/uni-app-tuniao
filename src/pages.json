{
	"easycom": {
		"custom": {
			"^tn-(.*)-(item|group)$": "@tuniao/tnui-vue3-uniapp/components/$1/src/$1-$2.vue",
			"^tn-(.*)": "@tuniao/tnui-vue3-uniapp/components/$1/src/$1.vue"
		  }
	},
	"pages": [
		{
			"path": "pages/home/index",
			"name":"Home",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/login/index",
			"name":"Login",
			"style": {
				"navigationBarTitleText": "登陆"
			}
		},

		{
			"path": "pages/user/index",
			"name":"user",
			"style": {
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/test/index",
			"name":"test",
			"style": {
				"navigationBarTitleText": "测试"
			}
		}
	],
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#435950",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/home/index",
				"iconPath": "static/tabbar/shouye.png",
				"selectedIconPath": "static/tabbar/shouye-copy.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/user/index",
				"iconPath": "static/tabbar/wode-copy.png",
				"selectedIconPath": "static/tabbar/wode.png",
				"text": "我的"
			}
		]
	},
	"subPackages": [ //大的文件管理
		{
			"root": "subPages/subUser",  // pages 同级分包目录
			"pages": [
				{
					"path": "about/index",
					"style": {
						"navigationBarTitleText": "关于我的"
					}
				},
				{
					"path": "vip/index",
					"style": {
						"navigationBarTitleText": "vip"
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
